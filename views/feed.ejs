<% include ./partials/header.ejs %>


<div class="text-white w-full min-h-screen bg-zinc-800 px-10 py-8">
   <div class="cards flex flex-wrap gap-10">
  <% posts.forEach(function(elem){%>
    <div class="card w-32">
        <div class="w-full h-40 rounded-lg ">
            <img class="w-full h-full bg-zinc-800 object-cover" src="/images/uploads/<%= elem.image %>" alt="">
        </div>
        <h5 class="mt-2 text-sm"><%= elem.title %></h5>
        <h5 class="text-xs opacity-500"><%= elem.user.username %></h5>
    </div>
   <%})%> 
   </div>
</div>


<% include ./partials/footer.ejs %>
